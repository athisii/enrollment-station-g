                </AnchorPane>

                <AnchorPane layoutX="192.0" layoutY="180.0" prefHeight="231.0" prefWidth="205.0"
                            style="-fx-background-color: #fff;">

                    <ImageView fitHeight="125.0" fitWidth="100.0" layoutX="46.0" layoutY="29.0" pickOnBounds="true"
                               preserveRatio="true">

                        <Image url="@../img/settings_new.png"/>

                    </ImageView>
                    <Button layoutX="30.0" layoutY="162.0" maxHeight="35.0" maxWidth="150.0" minHeight="35.0"
                            minWidth="150.0" mnemonicParsing="false" onAction="#serverConfig" prefHeight="35.0"
                            prefWidth="150.0" style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;"
                            text="SERVER CONFIG" textFill="WHITE">
                        <font>
                            <Font name="System Bold" size="14.0"/>
                        </font>
                    </Button>

                </AnchorPane>
                <AnchorPane layoutX="407.0" layoutY="180.0" prefHeight="231.0" prefWidth="205.0"
                            style="-fx-background-color: #fff;">

                    <ImageView fitHeight="86.0" fitWidth="98.0" layoutX="50.0" layoutY="52.0" pickOnBounds="true"
                               preserveRatio="true">

                        <Image url="@../img/navy_blue_check.png"/>

                    </ImageView>
                    <Button layoutX="30.0" layoutY="162.0" maxHeight="35.0" maxWidth="150.0" minHeight="35.0"
                            minWidth="150.0" mnemonicParsing="false" onAction="#deviceCheck" prefHeight="35.0"
                            prefWidth="150.0" style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;"
                            text="DEVICE CHECK" textFill="WHITE">
                        <font>
                            <Font name="System Bold" size="14.0"/>
                        </font>
                    </Button>

                </AnchorPane>
                <AnchorPane layoutX="621.0" layoutY="179.0" prefHeight="231.0" prefWidth="205.0"
