                        <Image url="@../img/licence_info.png"/>

                    </ImageView>
                    <Button layoutX="30.0" layoutY="162.0" maxHeight="35.0" maxWidth="150.0" minHeight="35.0"
                            minWidth="150.0" mnemonicParsing="false" onAction="#licenseInfo" prefHeight="35.0"
                            prefWidth="150.0" style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;"
                            text="LICENSE INFO" textFill="WHITE">
                        <font>
                            <Font name="System Bold" size="14.0"/>
                        </font>
                    </Button>

                </AnchorPane>
                <Label fx:id="messageLabel" alignment="CENTER" layoutX="260.0" layoutY="630.0" prefHeight="33.0"
                       prefWidth="487.0">
                    <font>
                        <Font name="System Bold" size="15.0"/>
                    </font>
                </Label>
                <AnchorPane layoutX="190.0" layoutY="400.0" prefHeight="231.0" prefWidth="205.0"
                            style="-fx-background-color: #fff;">

                    <ImageView fitHeight="125.0" fitWidth="100.0" layoutX="46.0" layoutY="39.0" pickOnBounds="true"
                               preserveRatio="true">

                        <Image url="@../img/reset.png"/>

                    </ImageView>
                    <Button layoutX="30.0" layoutY="162.0" maxHeight="35.0" maxWidth="150.0" minHeight="35.0"
