                        <font>
                            <Font name="System Bold" size="14.0"/>
                        </font>
                    </Button>

                </AnchorPane>
                <AnchorPane layoutX="621.0" layoutY="400.0" prefHeight="231.0" prefWidth="205.0"
                            style="-fx-background-color: #fff;">

                    <Label layoutX="40.0" layoutY="80.0" maxHeight="35.0" maxWidth="150.0" text="FP LIVENESS">
                        <font>
                            <Font name="System Bold" size="14.0"/>
                        </font>
                    </Label>
                    <TextField fx:id="liveFpTextField" alignment="center" editable="false" layoutX="69.0"
                               layoutY="110.0" maxHeight="35.0" maxWidth="60.0">
                        <font>
                            <Font name="System Bold" size="16.0"/>
                        </font>
                    </TextField>

                    <Button fx:id="liveFpBtn" layoutX="17.0" layoutY="162.0" maxHeight="35.0" maxWidth="181.0"
                            minHeight="35.0" minWidth="150.0" mnemonicParsing="false" prefHeight="35.0"
                            prefWidth="154.0" style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;"
                            text="EDIT" textFill="WHITE">
                        <font>
                            <Font name="System Bold" size="14.0"/>
                        </font>
                    </Button>
                </AnchorPane>

                <AnchorPane fx:id="confirmPane" layoutX="80.0" layoutY="640.0" prefHeight="67.0" prefWidth="863.0"
                            style="-fx-border-color: #04127b;" visible="false">

                    <Button layoutX="271.0" layoutY="32.0" maxHeight="35.0" maxWidth="150.0" minHeight="28.0"
                            minWidth="150.0" mnemonicParsing="false" onAction="#restart" prefHeight="28.0"
                            prefWidth="150.0" style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;"
                            text="Yes" textFill="WHITE">
                        <font>
                            <Font name="System Bold" size="14.0"/>
                        </font>
                    </Button>
                    <Text fill="#04127b" layoutX="209.0" layoutY="24.0" strokeType="OUTSIDE" strokeWidth="0.0"
                          text="Click 'Yes' to Restart " textAlignment="JUSTIFY" visible="false">
                        <font>
                            <Font size="18.0"/>
                        </font>
                    </Text>
                    <Button layoutX="460.0" layoutY="31.0" maxHeight="35.0" maxWidth="150.0" minHeight="28.0"
                            minWidth="150.0" mnemonicParsing="false" onAction="#stayBack" prefHeight="28.0"
                            prefWidth="150.0" style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;"
                            text="No" textFill="WHITE">
                        <font>
                            <Font name="System Bold" size="14.0"/>
                        </font>
                    </Button>
                    <Label alignment="CENTER" layoutX="140.0" prefHeight="16.0" prefWidth="588.0"
                           text="Click &quot;Yes&quot; to Restart" textFill="#04127b">
                        <font>
                            <Font size="18.0"/>
                        </font>
                    </Label>

                    <opaqueInsets>
                        <Insets top="-10.0"/>
                    </opaqueInsets>
                </AnchorPane>

                <AnchorPane layoutY="728.0" prefHeight="40.0" prefWidth="1024.0"
                            style="-fx-background-color: #1990ea;"/>

            </AnchorPane>
