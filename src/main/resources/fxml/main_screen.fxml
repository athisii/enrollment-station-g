<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="768.0" prefWidth="1024.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cdac.enrollmentstation.controller.PrimaryController">

    <BorderPane prefHeight="778.0" prefWidth="1024.0">
        <top>
            <AnchorPane prefHeight="768.0" prefWidth="1024.0" style="-fx-background-color: #fff;" BorderPane.alignment="CENTER">

                <AnchorPane prefHeight="100.0" prefWidth="1024.0" style="-fx-background-color: #1990ea;">

                    <ImageView fitHeight="91.0" fitWidth="363.0" layoutX="20.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true">

                        <Image url="@../img/navy_logo.png" />

                    </ImageView>
                </AnchorPane>

                <ImageView fitHeight="47.0" fitWidth="48.0" layoutX="21.0" layoutY="112.0" pickOnBounds="true" preserveRatio="true">

                    <Image url="@../img/home.png" />

                </ImageView>
                <Label layoutX="66.0" layoutY="131.0" prefHeight="27.0" prefWidth="77.0" text="HOME">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Label>

                <ImageView fitHeight="45.0" fitWidth="48.0" layoutX="880.0" layoutY="110.0" pickOnBounds="true" preserveRatio="true">

                    <Image url="@../img/setting_first.png" />

                </ImageView>
                <Label layoutX="930.0" layoutY="120.0" onMouseClicked="#onSettings" prefHeight="27.0" prefWidth="88.0" style="-fx-cursor: hand;" text="SETTINGS" underline="true">
                    <font>
                        <Font size="16.0" />
                    </font>
                </Label>
                <AnchorPane layoutX="179.0" layoutY="234.0" prefHeight="352.0" prefWidth="337.0">

                    <ImageView fitHeight="210.0" fitWidth="213.0" layoutX="43.0" layoutY="18.0" pickOnBounds="true" preserveRatio="true">

                        <Image url="@../img/bio_first.png" />

                    </ImageView>
                    <Button layoutX="26.0" layoutY="259.0" mnemonicParsing="false" onAction="#showEnrollmentHome" prefHeight="37.0" prefWidth="265.0" style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;" text="BIOMETRIC ENROLMENT" textFill="WHITE">
                        <font>
                            <Font size="16.0" />
                        </font>
                    </Button>

                </AnchorPane>
                <AnchorPane layoutX="529.0" layoutY="234.0" prefHeight="341.0" prefWidth="340.0">

                    <ImageView fitHeight="213.0" fitWidth="261.0" layoutX="71.0" layoutY="9.0" pickOnBounds="true" preserveRatio="true">

                        <Image url="@../img/token_first.png" />

                    </ImageView>
                    <Button layoutX="80.0" layoutY="259.0" mnemonicParsing="false" onAction="#showContract" prefHeight="37.0" prefWidth="203.0" style="-fx-background-color: #04127b; -fx-background-radius: 20 20 20 20;" text="TOKEN ISSUANCE" textFill="WHITE">
                        <font>
                            <Font size="16.0" />
                        </font>
                    </Button>

                </AnchorPane>
                <AnchorPane layoutY="730.0" prefHeight="40.0" prefWidth="1024.0" style="-fx-background-color: #1990ea;">
                    <Label fx:id="version" alignment="CENTER" layoutX="505.0" layoutY="10.0" textFill="#f8f5f5" />
                </AnchorPane>
            </AnchorPane>
        </top>
    </BorderPane>

</VBox>
